{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": 2020
  },
  "rules": {
    "no-console": "off",
    "no-underscore-dangle":  ["error", { "allow": ["_pool", "_service", "_validator", "_authenticationsService", "_usersService", "_tokenManager", "_songService", "_exports", "_playlistService"] }]
  }
}
